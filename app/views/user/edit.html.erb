<h1>Edit Your Account</h1>
<%- if current_user.avatar %>
<%= image_tag current_user.avatar.url(:medium) %>
<% end %>
<%= form_for current_user, :url => user_path(current_user) , :html => { :multipart => true, :method => :put } do |f|%>
	 <%= f.label :email %>
	 <%= f.text_field :email %>
	 <%= f.label :avatar %>
	 <%= f.file_field :avatar %>
	 <%= f.button :submit %>
<% end %>

